# Run the web-service in interactive mode as a daemon (daemon is optional) 
docker run -d -it --rm --name secret devopsdockeruh/simple-web-service:ubuntu

# Spawn a process inside the running container to enter it (ex. bash)
# However, here we will use tail -f ./text.log to complete the exercise
docker exec -it secret tail -f ./text.log

